
public class UltraEmojiCompat {

	public static void main(String[] args) {
		// [] é um vetor que no caso será 6 opções de lutadores já que começa do 0 até o
		// 5
		Lutador l[] = new Lutador[6];

		l[0] = new Lutador("Marcos paulo", "Brasil", 30, 1.75f, 68.9f, 20, 2, 1);
		l[1] = new Lutador("Cayo", "França", 20, 1.65f, 60.4f, 10, 60, 4);

		Luta UEC01 = new Luta();
		UEC01.marcarLuta(l[0], l[1]);
		UEC01.lutar();
		l[0].status();
		l[1].status();
	}
}
